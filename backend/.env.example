# Database
DATABASE_URL=postgresql://user:password@localhost:5432/lease_abstraction

# Anthropic API
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# AWS S3 Storage
# For local development without S3, set AWS_ACCESS_KEY_ID=test to use local file storage
# For production, use real AWS credentials
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_REGION=us-east-1
S3_BUCKET_NAME=lease-abstraction-pdfs

# Application
ENVIRONMENT=development
DEBUG=true
SECRET_KEY=your-secret-key-here-change-in-production

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# API Settings
MAX_UPLOAD_SIZE_MB=50
ALLOWED_FILE_TYPES=application/pdf
